{
  "name": "mitirrli/laravel-skeleton",
  "description": "A Laravel starter project template.",
  "keywords": [
    "spa",
    "laravel",
    "api"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "bensampo/laravel-enum": "^3.3",
    "fideloper/proxy": "^4.4.1",
    "fruitcake/laravel-cors": "^2.0.3",
    "guzzlehttp/guzzle": "^7.2.0",
    "laravel/framework": "^8.14.0",
    "laravel/sanctum": "^2.8.0",
    "laravel/tinker": "^2.5.0",
    "laravelcollective/annotations": "^8.0",
    "nuwave/lighthouse": "^5.8",
    "overtrue/laravel-lang": "^4.1.1",
    "overtrue/laravel-options": "^2.0.0",
    "predis/predis": "^1.1.6",
    "thecodingmachine/phpstan-safe-rule": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.10",
    "beyondcode/laravel-dump-server": "^1.6.0",
    "brainmaestro/composer-git-hooks": "^2.8.3",
    "doctrine/dbal": "^3.0.0",
    "ekino/phpstan-banned-code": "^0.4.0",
    "facade/ignition": "^2.5.0",
    "friendsofphp/php-cs-fixer": "^2.16.7",
    "fzaninotto/faker": "^1.9.1",
    "mll-lab/laravel-graphql-playground": "^2.5",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.1.0",
    "nunomaduro/larastan": "^0.7.5",
    "overtrue/laravel-query-logger": "^2.0.1",
    "phpstan/phpstan": "^0.12.87",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-mockery": "^0.12.13",
    "phpunit/phpunit": "^9.4.3",
    "vimeo/psalm": "^4.7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "./bootstrap/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "extra": {
      "laravel": {
        "dont-discover": [
          "barryvdh/laravel-ide-helper"
        ]
      }
    },
    "hooks": {
      "pre-commit": [
        "composer test",
        "composer check-style",
        "composer psalm",
        "composer stan"
      ],
      "pre-push": [
        "composer test",
        "composer check-style"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "cghooks update",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-merge": "composer install",
    "post-install-cmd": [
      "cghooks add --ignore-lock",
      "cghooks update"
    ],
    "update-project": [
      "@php artisan ide-helper:models -N",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "cghooks": "vendor/bin/cghooks",
    "check-style": "php-cs-fixer fix --using-cache=no --diff --config=.php_cs --dry-run --ansi",
    "cs-fix": "php-cs-fixer fix --using-cache=no --config=.php_cs --ansi",
    "test": "@php artisan test",
    "stan": "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=1G --verbose",
    "psalm": "vendor/bin/psalm --show-info=true"
  },
  "scripts-descriptions": {
    "test": "Run all tests.",
    "check-style": "Run style checks (only dry run - no fixing!).",
    "fix-style": "Run style checks and fix violations."
  }
}
